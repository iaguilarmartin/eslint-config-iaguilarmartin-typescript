{
  "name": "eslint-config-iaguilarmartin-typescript",
  "version": "1.0.0",
  "description": "My personal ESLint configuration for TypeScript projects",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "eslint": "npm run eslint:base && npm run eslint:react",
    "eslint:base": "eslint -c ./configs/.eslintrc .",
    "eslint:react": "eslint -c ./configs/.react.eslintrc .",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iaguilarmartin/eslint-config-iaguilarmartin-typescript.git"
  },
  "author": "Iván Aguilar Martín",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iaguilarmartin/eslint-config-iaguilarmartin-typescript/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/iaguilarmartin/eslint-config-iaguilarmartin-typescript#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "2.0.5",
    "typescript": "3.9.3"
  },
  "peerDependencies": {},
  "dependencies": {}
}
